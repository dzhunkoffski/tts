{
    "name": "fast_speech_v1",
    "n_gpu": 1,
    "preprocessing": {
        "sr": 16000
    },
    "augmentations": {
        "wave": [],
        "spectorgram": []
    },
    "arch": {
        "type": "FastSpeechV1",
        "args": {
            "max_len": 5000,
            "pad_idx": 0,
            "n_blocks": 6,
            "n_heads": 2,
            "fft_kernel": 3,
            "lr_kernel": 3,
            "embed_dim": 384,
            "n_mels": 80
        }
    },
    "data": {
        "train": {
            "batch_size": 10,
            "num_workers": 4,
            "datasets": [
                {
                    "type": "LJSpeechDataset",
                    "args": {
                        "dataset_path": "ok",
                        "is_train": true,
                        "train_size": 1.0,
                        "text_cleaners": ["english_cleaners"],
                        "mel_spec_path": "ok",
                        "alignment_path": "ok"
                    }
                }
            ]
        }
    }
}